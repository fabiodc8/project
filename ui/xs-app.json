{
  "welcomeFile": "/ui/index.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
{
      "source": "^/node_dest/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "nodejs",
      "csrfProtection": false
    },
    {
      "source": "^/ui/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
     
  ]
}